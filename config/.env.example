# Exemplo de arquivo .env - Renomeie para .env para uso local
# NÃO comite o arquivo .env no repositório Git!

# Magic SDK
MAGIC_PUBLIC_KEY=pk_live_XXXXXXXXXXXXXXXX
MAGIC_SECRET_KEY=sk_live_XXXXXXXXXXXXXXXX  # Substitua pelo seu valor real

# API Key
API_KEY=sua_api_key_aqui

# Google Sheets
GOOGLE_SHEETS_CREDENTIALS=base64_encoded_credentials_here
GOOGLE_SHEETS_SPREADSHEET_ID=seu_spreadsheet_id_aqui
